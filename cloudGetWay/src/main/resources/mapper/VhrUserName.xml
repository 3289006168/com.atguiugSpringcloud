<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.Mapper.HrUserMapper">
    <select id="getByName" resultType="UserInfo" parameterType="string">
        select * from vhr.user_info where vhr.user_info.user_name = #{name}
    </select>
</mapper>